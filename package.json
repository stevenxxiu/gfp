{
  "name": "google_search_filter_plus",
  "version": "1.0.0",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-es2015-block-scoped-functions": "^6.1.18",
    "babel-plugin-transform-es2015-classes": "^6.2.2",
    "babel-plugin-transform-es2015-constants": "^6.1.4",
    "babel-plugin-transform-es2015-destructuring": "^6.1.18",
    "babel-plugin-transform-es2015-function-name": "^6.1.18",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.2.0",
    "babel-plugin-transform-es2015-object-super": "^6.1.18",
    "babel-plugin-transform-es2015-parameters": "^6.1.18",
    "babel-plugin-transform-es2015-unicode-regex": "^6.1.18",
    "babel-preset-es2015-without-regenerator": "^1.0.1",
    "chai": "^3.4.1",
    "css-loader": "^0.23.0",
    "file-loader": "^0.8.4",
    "firefox-profile": "^0.3.11",
    "gulp": "^3.9.0",
    "gulp-add-src": "^0.2.0",
    "gulp-continuous-concat": "^0.1.1",
    "html-loader": "^0.3.0",
    "isparta": "^4.0.0",
    "isparta-instrumenter-loader": "^1.0.0",
    "karma": "^0.13.15",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "merge": "^1.2.0",
    "mocha": "^2.3.4",
    "node-sass": "^3.4.2",
    "open": "^0.0.5",
    "sass-loader": "^3.1.2",
    "sinon": "^1.17.2",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.8",
    "webpack-stream": "^2.1.1"
  },
  "eslintConfig": {
    "root": true,
    "ecmaFeatures": {
      "modules": true
    },
    "rules": {
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "max-len": [
        1,
        120,
        4
      ],
      "no-console": 0,
      "no-empty": 0,
      "no-unused-vars": [
        2,
        {
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ],
      "quotes": [
        2,
        "single"
      ],
      "semi": [
        2,
        "never"
      ]
    },
    "env": {
      "es6": true,
      "browser": true,
      "mocha": true
    },
    "globals": {
      "assert": false,
      "require": false,
      "sinon": false
    },
    "extends": "eslint:recommended"
  }
}
