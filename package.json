{
  "name": "google_search_filter_plus",
  "version": "1.0.0",
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-istanbul": "^3.0.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "file-loader": "^0.9.0",
    "firefox-profile": "^0.4.4",
    "gulp": "^3.9.1",
    "gulp-add-src": "^0.2.0",
    "gulp-continuous-concat": "^0.1.1",
    "html-loader": "^0.4.4",
    "istanbul": "^0.4.5",
    "jsdom": "^9.8.3",
    "jsdom-global": "^2.1.0",
    "mocha": "^3.2.0",
    "node-sass": "^3.13.0",
    "postcss-loader": "^1.2.0",
    "sass-loader": "^4.0.2",
    "sinon": "^2.0.0-pre.4",
    "through-gulp": "^0.5.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.27",
    "webpack-stream": "github:jeroennoten/webpack-stream#patch-1"
  },
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "rules": {
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "max-len": [
        "error",
        120
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "no-console": "off",
      "no-constant-condition": [
        "error",
        {
          "checkLoops": false
        }
      ],
      "no-empty": "off",
      "no-unused-vars": [
        "error",
        {
          "args": "after-used",
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "never"
      ]
    },
    "env": {
      "es6": true,
      "amd": true,
      "browser": true,
      "mocha": true
    },
    "globals": {
      "global": true,
      "GM_addStyle": true,
      "GM_getResourceText": true,
      "GM_getResourceURL": true,
      "GM_getValue": true,
      "GM_setValue": true,
      "GM_registerMenuCommand": true
    },
    "extends": "eslint:recommended"
  }
}
